    {
      "id":"004",
      "title":{
        "jp":"Haibara-kun no Tsuyokute Seishun New Game",
        "eng":"Haibara’s Teenage New Game+"
      },
      "images":{
        "cover":"https://warlock03.github.io/..../images/Haibaras-Teenage-New-Game/Haiharakuns-Strong-and-Youthful-New-Game.jpg",
